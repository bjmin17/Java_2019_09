package com.biz.classes;

import com.biz.classes.grade.GradeNew;

public class GradeMain_01 {

	public static void main(String[] args) {

		GradeNew grade01 = new GradeNew();

		// grade01 객체의 멤버변수에 값을 setting 한다.
		// 변수에 값을 저장
		grade01.strNum = "001";
		grade01.strName = "홍길동";
		
		grade01.intKor = 90;
		grade01.intEng = 88;
		grade01.intMath = 77;
		
		
		GradeNew grade02 = new GradeNew();
		grade02.strNum = "002";
		grade02.strName = "이몽룡";
		
		grade02.intEng = 88;
		grade02.intMath = 66;
		grade02.intKor = 78;
		
		GradeNew grade03 = new GradeNew();
		grade03.strNum = "003";
		grade03.strName = "이몽룡";
		
		grade03.intEng = 98;
		grade03.intMath = 78;
		grade03.intKor = 68;
		
		System.out.println("001 학생의 성적");
		System.out.println("국어 : " + grade01.intKor);
		System.out.println("영어 : " + grade01.intEng);
		System.out.println("수학 : " + grade01.intMath);
		
		iny inySum = grade01.intKor + grade01.intEng + grade01.intMath;
	}

}
