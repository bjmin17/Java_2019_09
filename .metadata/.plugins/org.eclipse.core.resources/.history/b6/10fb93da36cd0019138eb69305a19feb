package com.biz.arrays.service;

public class MethodService {

	private int intSum;
	/*
	 * method를 정의할 때
	 * (method 코드를 작성할 때)
	 * void method() : void 키워드가 붙은 method로 return 문이 있거나 없거나
	 */
	
	public void consoleView() { //void라고 앞에 있으면 return문이 있어도 되고 없어도 되고
		return ; //비어있으면 return;이라는 코드가 들어있음.(consoleView라는 메서드가 끝났다는 의미.)
	}
	
	public int numRet() {
		return 0;
	}
	
	public String strRet() {
		return "";
	}
	
	public int getIntSum() {
		return intSum;
	}
	
}
