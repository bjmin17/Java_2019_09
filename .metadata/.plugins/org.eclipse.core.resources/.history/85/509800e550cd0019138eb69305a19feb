package com.biz.arrays.exec;

import com.biz.arrays.service.MethodService;

public class Method_05 {

	public static void main(String[] args) {
		
		MethodService ms = new MethodService();
		
		ms.add(); //매개변수가 없는 method
		ms.add(100); //매개변수 1개 : 100
		ms.add(30, 40); //두개의 매개변수 합 : 70
		ms.add(30.f, 40.f); //두 실수의 합 : 70.000000

	}
}
