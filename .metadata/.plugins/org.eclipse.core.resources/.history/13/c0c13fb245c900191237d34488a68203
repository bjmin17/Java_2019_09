package com.biz.classes;

public class String_10 {

	public static void main(String[] args) {
		
		/*
		 * 1 부터 100까지 숫자 중에서 
		 * 7의 배수가 2번째로 나타나면
		 * 그 값을 표시하시오
		 */
		
		int num = 2;
		for(int i = 1 ; i < 100 ; i++) {
			if(i % 7 ==0)
				System.out.println(i);
			
				
				
			
			
			
			if(i % 7 == 0 && i == 7 * num) {
				System.out.println(i);
				}
			
		}
	}
}
