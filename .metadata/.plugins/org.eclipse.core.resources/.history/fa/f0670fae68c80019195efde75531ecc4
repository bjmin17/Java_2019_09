package com.biz.for_if;

public class If_03 {

	public static void main(String[] args) {

		int num = 32;

		if (num % 2 == 0)
			System.out.println(num + "는 짝수"); //32는 짝수

		if (num % 2 != 0) {
			System.out.println(num + "는 홀수");
		}
		
		/*
		 * 변수 i에 0부터 100까지 숫자를 1개씩 담아서...
		 */
		for(int i = 0; i <=100 ; i++) {
			if(i % 2 == 0)
		}

	}

}
