package com.biz.classes.service;

import java.util.Random;

public class ArrayScanService {

	private int[] nums ;
	
	public ArrayScanService(int length) {
		
		Random rnd = new Random();
		
		nums = new int[length];
		for(int i = 0 ; i < nums.length; i++) {
			nums[i] = rnd.nextInt(10) + 1;
		}
	}
}
