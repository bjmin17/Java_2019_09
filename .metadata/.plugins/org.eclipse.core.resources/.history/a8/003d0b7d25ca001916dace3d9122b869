package com.biz.model;

public class AddressVO {

	//1.주수록 필수정보 이름, 전화번호, 주소, 관계
	//2. 가. 주소정보 확인 기능
	
	public String strName;
	public String strNum;
	public String strAdd;
	public String strRel;
	
	public void view() {
		System.out.printf("%s\t%s\t%s\t%s\n",
				strName,
				strNum,
				strAdd,
				strRel);
	}
}
