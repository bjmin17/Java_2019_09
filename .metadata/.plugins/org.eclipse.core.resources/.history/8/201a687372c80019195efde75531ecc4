package com.biz.for_if;

public class If_07 {

	public static void main(String[] args) {

		int num = 66;
		
		// 다중조건, && : AND 좌우 조건이 모두 true일 때만 결과가 true
		if(num % 3 == 0 && num % 6 == 0) {
			System.out.println(num + "는 3의 배수이며, 6의 배수이다.");
		}
		
		
		// || : OR 좌우 조건 중 1개만 true이면 결과가 true
		if(num % 3 == 0 || num % 6 == 0)
			System.out.println(num + "는 3의 배수이며, 6의 배수이다.");

		
	}

}
